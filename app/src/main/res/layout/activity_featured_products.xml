<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CAE2ED"
    tools:context=".FeaturedProducts">

    <TextView
        android:id="@+id/featured_products_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="59dp"
        android:fontFamily="@font/alata"
        android:text="Featured Products"
        android:textColor="#000000"
        android:textSize="96px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView2" />

    <SearchView
        android:id="@+id/searchView2"
        android:layout_width="276dp"
        android:layout_height="40dp"
        android:layout_marginStart="119dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="#ffffff"
        android:hint="Search"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/smartTVOne"
        android:layout_width="161dp"
        android:layout_height="150dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="46dp"
        android:onClick="productOne"
        app:layout_constraintEnd_toStartOf="@+id/smartTVTwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/featured_products_heading"
        app:srcCompat="@drawable/latest_television_one" />

    <ImageView
        android:id="@+id/smartTVTwo"
        android:layout_width="163dp"
        android:layout_height="150dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="36dp"
        android:onClick="productTwo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/featured_products_heading"
        app:srcCompat="@drawable/latest_television_two" />

    <ImageView
        android:id="@+id/laptop1"
        android:layout_width="177dp"
        android:layout_height="175dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="115dp"
        android:layout_marginEnd="19dp"
        android:onClick="productThree"
        app:layout_constraintEnd_toStartOf="@+id/laptop2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/smartTVOne"
        app:srcCompat="@drawable/laptop1" />

    <ImageView
        android:id="@+id/laptop2"
        android:layout_width="180dp"
        android:layout_height="169dp"
        android:layout_marginTop="115dp"
        android:layout_marginEnd="19dp"
        android:onClick="productFour"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/smartTVTwo"
        app:srcCompat="@drawable/laptop2" />

    <Button
        android:id="@+id/GoBackButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="306dp"
        android:backgroundTint="#ff0000"
        android:onClick="BackHomePage"
        android:text="Back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/laptop1" />

    <TextView
        android:id="@+id/pro5_name"
        android:layout_width="158dp"
        android:layout_height="25dp"
        android:layout_marginStart="5dp"
        android:text="Toshiba Smart Fire TV"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/smartTVOne" />

    <TextView
        android:id="@+id/pro5_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:text="£395.99"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pro5_name" />

    <TextView
        android:id="@+id/pro5_vat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="20% VAT"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/pro5_price"
        app:layout_constraintTop_toBottomOf="@+id/pro5_name" />

    <TextView
        android:id="@+id/pro5_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="13dp"
        android:text="FI4959"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pro5_price" />

    <TextView
        android:id="@+id/pro6_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="53dp"
        android:text="Cello Smart Android TV"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pro5_name"
        app:layout_constraintTop_toBottomOf="@+id/smartTVTwo" />

    <TextView
        android:id="@+id/pro6_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginTop="10dp"
        android:text="£350.00"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/pro5_vat"
        app:layout_constraintTop_toBottomOf="@+id/pro6_name" />

    <TextView
        android:id="@+id/pro6_vat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="77dp"
        android:text="20% VAT"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pro6_price"
        app:layout_constraintTop_toBottomOf="@+id/pro6_name" />

    <TextView
        android:id="@+id/pro6_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="151dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="144dp"
        android:text="MC84859"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pro5_code"
        app:layout_constraintTop_toBottomOf="@+id/pro6_price" />

    <TextView
        android:id="@+id/pro7_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="4dp"
        android:text="Acer Aspire 5 Laptop"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/laptop1" />

    <TextView
        android:id="@+id/pro7_vat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:text="£295.00"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/pro7_code"
        app:layout_constraintTop_toBottomOf="@+id/pro7_name" />

    <TextView
        android:id="@+id/pro7_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="20% VAT"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pro7_name" />

    <TextView
        android:id="@+id/pro7_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="4dp"
        android:text="ZHF8483"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pro7_price" />

    <TextView
        android:id="@+id/pro8_name"
        android:layout_width="174dp"
        android:layout_height="36dp"
        android:layout_marginStart="13dp"
        android:text="Jumper Windows 11 Laptop"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/laptop1"
        app:layout_constraintTop_toBottomOf="@+id/laptop2" />

    <TextView
        android:id="@+id/pro8_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="62dp"
        android:text="£350.99"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/pro7_vat"
        app:layout_constraintTop_toBottomOf="@+id/pro8_name" />

    <TextView
        android:id="@+id/pro8_vat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:text="20% VAT"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/pro8_price"
        app:layout_constraintTop_toBottomOf="@+id/pro8_name" />

    <TextView
        android:id="@+id/pro8_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="6dp"
        android:text="TY8499"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/GoBackButton"
        app:layout_constraintTop_toBottomOf="@+id/pro8_price" />
</androidx.constraintlayout.widget.ConstraintLayout>