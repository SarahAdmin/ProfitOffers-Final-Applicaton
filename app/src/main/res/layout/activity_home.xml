<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CAE2ED"
    tools:context=".Home">

    <TextView
        android:id="@+id/informationOne"
        android:layout_width="217dp"
        android:layout_height="129dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="18dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="It is a best cash and carry wholesale in Tolworth and Kingston Upon Thames since 2022.
ProfitOffers has special promotions and discounts on bulk and featured products including VAT."
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iconOne"
        android:layout_width="84dp"
        android:layout_height="77dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="52dp"
        android:onClick="EnterBasket"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/promotions_heading"
        app:srcCompat="@drawable/basket_icon_1" />

    <ImageView
        android:id="@+id/iconThree"
        android:layout_width="85dp"
        android:layout_height="84dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="46dp"
        android:onClick="EnterMembership"
        app:layout_constraintStart_toEndOf="@+id/iconTwo"
        app:layout_constraintTop_toBottomOf="@+id/product_heading"
        app:srcCompat="@drawable/membership_icon_1" />

    <ImageView
        android:id="@+id/iconTwo"
        android:layout_width="89dp"
        android:layout_height="78dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="82dp"
        android:onClick="EnterAccount"
        app:layout_constraintStart_toEndOf="@+id/iconOne"
        app:layout_constraintTop_toBottomOf="@+id/offer_icon"
        app:srcCompat="@drawable/user_icon_1" />

    <ImageView
        android:id="@+id/iconFour"
        android:layout_width="90dp"
        android:layout_height="91dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="7dp"
        android:onClick="EnterLocation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/productTwo"
        app:srcCompat="@drawable/location_icon__1" />

    <TextView
        android:id="@+id/basket_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="42dp"
        android:text="Basket"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/account_heading"
        app:layout_constraintTop_toBottomOf="@+id/iconOne" />

    <TextView
        android:id="@+id/account_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="25dp"
        android:text="Account"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/membership_heading"
        app:layout_constraintTop_toBottomOf="@+id/iconTwo" />

    <TextView
        android:id="@+id/membership_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:text="My Membership"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/location_heading"
        app:layout_constraintTop_toBottomOf="@+id/iconThree" />

    <TextView
        android:id="@+id/location_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="12dp"
        android:text="Branch Location"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iconFour" />

    <ImageView
        android:id="@+id/productOne"
        android:layout_width="258dp"
        android:layout_height="153dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="9dp"
        android:onClick="FeaturedPage"
        app:layout_constraintBottom_toTopOf="@+id/featured_product_heading"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/informationOne"
        app:srcCompat="@drawable/featured_product_one" />

    <TextView
        android:id="@+id/featured_product_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:layout_marginTop="9dp"
        android:text="Featured Products"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/productOne" />

    <ImageView
        android:id="@+id/offer_icon"
        android:layout_width="175dp"
        android:layout_height="163dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="11dp"
        android:onClick="OfferPage"
        app:layout_constraintBottom_toTopOf="@+id/promotions_heading"
        app:layout_constraintEnd_toStartOf="@+id/productTwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/featured_product_heading"
        app:srcCompat="@drawable/offer_icon_1" />

    <TextView
        android:id="@+id/promotions_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="11dp"
        android:text="Promotions"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/offer_icon" />

    <ImageView
        android:id="@+id/productTwo"
        android:layout_width="155dp"
        android:layout_height="165dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="47dp"
        android:onClick="ProductsPage"
        app:layout_constraintStart_toEndOf="@+id/offer_icon"
        app:layout_constraintTop_toBottomOf="@+id/productOne"
        app:srcCompat="@drawable/product_icon_1" />

    <TextView
        android:id="@+id/product_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="127dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="133dp"
        android:text="Products"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/promotions_heading"
        app:layout_constraintTop_toBottomOf="@+id/productTwo" />

</androidx.constraintlayout.widget.ConstraintLayout>