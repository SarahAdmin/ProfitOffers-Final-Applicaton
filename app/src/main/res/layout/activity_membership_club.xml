<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MembershipClub">

    <TextView
        android:id="@+id/membership_club_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="70dp"
        android:fontFamily="@font/alata"
        android:text="Membership Club"
        android:textColor="#000000"
        android:textSize="96px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="148dp"
        android:layout_height="173dp"
        app:srcCompat="@drawable/user_icon_1"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="134dp" />

    <TextView
        android:id="@+id/users_name"
        android:layout_width="153dp"
        android:layout_height="42dp"
        android:text=""
        android:textColor="#000000"
        android:textSize="70px"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="328dp" />

    <TextView
        android:id="@+id/membership_no_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Membership No."
        android:textColor="#000000"
        android:textSize="75px"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="398dp" />

    <TextView
        android:id="@+id/users_membership_no"
        android:layout_width="212dp"
        android:layout_height="44dp"
        android:text=""
        android:textColor="#000000"
        android:textSize="70px"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="450dp" />

    <ImageView
        android:id="@+id/back_arrow_2"
        android:layout_width="100dp"
        android:layout_height="109dp"
        android:onClick="BacktoHomePage1"
        app:srcCompat="@drawable/left_arrow__1"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="622dp" />
</androidx.constraintlayout.widget.ConstraintLayout>